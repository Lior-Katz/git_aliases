[alias]
	st = status
	cm = commit -m
	co = checkout
	br = branch
	cfg = !git config --file C:\\\\gitl\\\\alias4git\\\\gitalias.config
	dv = difftool -t vimdiff -y
	gl = cfg -l
	ac = commit -am
	se = !git rev-list --all | xargs git grep -F
	npp = "!start notepad++ "
	ff = "flow feature"
	nbr = git co -b

	map = log --pretty --oneline --graph
	catmerge = merge --strategy-option theirs
	cln = clean -fd
	dpcln = clean -fdx
[gitflow "branch"]
	master = main
	develop = develop
[gitflow "prefix"]
	feature = feature/
	bugfix = bugfix/
	release = release/
	hotfix = hotfix/
	support = support/
	versiontag = 
[gitflow "path"]
	hooks = .git/hooks
[help]
	autocorrect = 20
